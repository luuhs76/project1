#include <stdio.h>
int main(void)
{
	int i, j;
	for (i = 0; i < 2; i++)
	{
		for (j = 0; j < i+1; j++)
		{
			printf("*");
		}
		printf("\n");
	}
	printf("#*\n");
	for (i = 0; i < 3; i++)
	{
		for (j = 1; j < i+1;j++)
		{
			printf("#");
		}
		for (j = 0; j < 3 - i; j++)
		{
			printf("*");
		}
		printf("\n");
	}
	return 0;
}
